                    p.getInventory().addItem(melons);
                    p.getInventory().addItem(gdust);
                    p.getInventory().addItem(gpowder);
                    p.getInventory().addItem(rdust);

                    engineerTime.put(p, 48);
                    engineerTask.put(p, new BukkitRunnable() {

                        public void run() {

                            engineerTime.put(p, engineerTime.get(p) - 1);

                            if (engineerTime.get(p) == 0) {

                                engineerTime.remove(p);
                                engineerTask.remove(p);

                                cancel();

                            }

                        }

                    });

                    engineerTask.get(p).runTaskTimer(this, 72000, 72000);
            		
            	} else {
            		
            		p.closeInventory();
            		
            		p.sendMessage(ChatColor.RED + "(!) " + ChatColor.GRAY + "You do not have permissions for the " + ChatColor.UNDERLINE + "Engineer Kit" + ChatColor.GRAY + ".");
            		
            	}

            } else if (e.getCurrentItem().getItemMeta().getDisplayName().equalsIgnoreCase(ChatColor.DARK_RED + "" + ChatColor.BOLD + "Kit Dragon")) {



            } else if (e.getCurrentItem().getItemMeta().getDisplayName().equalsIgnoreCase(ChatColor.GREEN + "" + ChatColor.BOLD + "Kit Leprechaun")) {
            	
            	
            	
            } else if (e.getCurrentItem().getItemMeta().getDisplayName().equalsIgnoreCase(ChatColor.DARK_PURPLE + "" + ChatColor.BOLD + "Kit Ghoul")) {
            	
            	
            	
            } else if (e.getCurrentItem().getItemMeta().getDisplayName().equalsIgnoreCase(ChatColor.WHITE + "" + ChatColor.BOLD + "Kit Snowflake")) {
            	
            	
            	
            }

        }

    }

    public boolean onCommand(CommandSender sender, Command cmd, String label, String[] args) {

        Player p = (Player) sender;

        if (cmd.getName().equalsIgnoreCase("kit")) {

            kitMenu(p);

        }
        
        if (cmd.getName().equalsIgnoreCase("particles")) {
        	
        	particleMenu(p);
        	
        }

        return true;

    }

